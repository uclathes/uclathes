\newcommand\@maketitlepage{
   \begin {titlepage}
      \ColumnSave
      \normalsize \normalfont
      \ifdefined\pdfbookmark\pdfbookmark{Title Page}{titlepage}\fi
      \begin {center}
         \MakeUppercase{University of California} \\
         \normalsize Los Angeles \\
         \vfill
         \@titlesize \@title \\
         \vskip 48pt plus0pt minus18pt
         \normalsize \normalfont
         A \@thesisname\ submitted in partial satisfaction \\
         of the requirements for the degree \\
         \@degreename\if@department\ in \@department \fi\\
         \vskip 24pt plus0pt minus6pt
         by \\
         \vskip 24pt plus0pt minus6pt
         {\@author} \\
         \vfill
         \normalsize \normalfont \@degreeyear
      \end {center}
      \ColumnRestore
   \end {titlepage}
}

